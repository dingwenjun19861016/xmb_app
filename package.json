    "react-native-view-shot": "^4.0.3",
    "react-native-web": "^0.20.0"
  },
  "scripts": {
    "start": "expo start",
    "start:test": "EXPO_PUBLIC_API_ENV=test expo start",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "npx expo start --go",
    "web:test": "EXPO_PUBLIC_API_ENV=test npx expo start --go",
    "new": "npx expo start --clear",
    "build": "npx expo export --platform web --output-dir dist",
    "build:prod": "NODE_ENV=production npx expo export --platform web --output-dir dist",
    "build:test": "EXPO_PUBLIC_API_ENV=test NODE_ENV=production npx expo export --platform web --output-dir dist",
    "serve": "npx serve dist -s -l 3005",
    "prod": "yarn && npm run build:prod && pm2 start ecosystem.config.js --only xmb-app-web --env production",
    "prod:test": "EXPO_PUBLIC_API_ENV=test yarn && npm run build:test && pm2 start ecosystem.config.js --only xmb-app-web --env test",
    "prod:stop": "pm2 stop xmb-app-web",
    "prod:restart": "pm2 restart xmb-app-web",
    "prod:logs": "pm2 logs xmb-app-web",
    "prod:status": "pm2 status",
    "deploy": "./scripts/update-version.sh",
    "deploy:quick": "./scripts/update-version.sh && yarn prod"
  },
  "devDependencies": {
    "@react-native-community/cli": "^18.0.0",
    "@types/react": "~19.0.10",
    "@types/react-native": "^0.73.0",
    "serve": "^14.2.4",
    "typescript": "^5.3.3"
  },
  "name": "xmb_app",
  "version": "1.0.1-test"
}